container_commands:
  extend_proxy_timeout:
    command: |
      sed -i '/\s*location \/ {/c \
              location / { \
                  proxy_connect_timeout       300;\
                  proxy_send_timeout          300;\
                  proxy_read_timeout          300;\
                  send_timeout                300;\
              ' /etc/nginx/sites-available/elasticbeanstalk-nginx-docker-proxy.conf
